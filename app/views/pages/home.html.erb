  <div class="home-3">
    <div class="home-3_container">
      <h4><span>Products</span> that we have proudly built!</h4>
      <div class="home-3_container_cards">
        <% @projects.last(5).each do |project| %>
          <div class="home-3_card">
            <%= link_to project_path(project) do %>
              <% if project.images.attached? %>
                <%= image_tag project.images.first, alt: "london web development" %>
              <% else %>
                <%= image_tag 'https://images.pexels.com/photos/356079/pexels-photo-356079.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940' %>
              <% end %>
              <span>
                <div>
                  <p><%= project.title %></p>
                  <p><%= project.project_type %></p>
                </div>
                <button>Details</button>
              </span>
            <% end %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
